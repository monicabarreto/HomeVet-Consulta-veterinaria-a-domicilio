@model CadProfissionalModel

@{
            
}

<!-- Adicione as referências necessárias para o FullCalendar -->
<link href='fullcalendar.min.css' rel='stylesheet' />
<script src='fullcalendar.min.js'></script>

<script>
    $(document).ready(function () {
        // Inicialize o FullCalendar no elemento HTML com o ID 'calendario'
        $('#calendario').fullCalendar({
            // Configurações do calendário
            defaultView: 'month', // exibir mês como padrão
            editable: true, // permitir edição de eventos
            // Adicione seus eventos aqui
            events: [
                {
                    title: 'Evento 1',
                    start: '2024-04-19' // data do evento
                },
                {
                    title: 'Evento 2',
                    start: '2024-04-20' // data do evento
                },
                // Adicione mais eventos conforme necessário
            ],
            // Adicione mais opções conforme necessário
        });
    });
</script>
